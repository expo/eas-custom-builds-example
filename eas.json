{
  "cli": {
    "version": ">= 3.7.0"
  },
  "build": {
    "production": {},
    "test": {
      "withoutCredentials": true,
      "config": "test.yml"
    },
    "demo": {
      "withoutCredentials": true,
      "config": "demo.yml"
    },
    "vars": {
      "withoutCredentials": true,
      "env": {
        "TEST1": "abc123",
        "TEST2": "def456"
      },
      "config": "vars.yml"
    },
    "upload": {
      "withoutCredentials": true,
      "config": "upload.yml"
    },
    "functions": {
      "withoutCredentials": true,
      "config": "functions.yml"
    },
    "import": {
      "withoutCredentials": true,
      "config": "import.yml"
    },
    "customFunctions": {
      "withoutCredentials": true,
      "config": "custom-functions.yml"
    },
    "productionBuild": {
      "android": {
        "config": "production-build-android.yml"
      },
      "ios": {
        "config": "production-build-ios.yml"
      }
    },
    "developmentBuild": {
      "distribution": "internal",
      "android": {
        "config": "development-build-android.yml"
      },
      "ios": {
        "simulator": true,
        "config": "development-build-ios.yml"
      }
    }
  }
}
